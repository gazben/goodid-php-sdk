{
    "name": "goodid/goodid-php-sdk",
    "description": "GoodID SDK for PHP",
    "keywords": ["goodid", "sdk"],
    "type": "library",
    "homepage": "https://github.com/idandtrust/goodid-php-sdk",
    "license": "GoodID Platform",
    "require": {
        "php": ">=5.6",
        "ext-openssl": "*",
        "spomky-labs/jose": "^6.0",
        "paragonie/random_compat": "^1|^2",
        "jwadhams/json-logic-php": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "GoodID\\": "src/GoodID/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoodID\\": "tests/unit/GoodID/"
        }
    }
}
